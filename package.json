{
	"name": "nacc-home",
	"version": "0.1.0",
	"description": "National Aquatic Connectivity Collaborative",
	"author": "Brendan C. Ward <bcward@astutespruce.com>",
	"license": "MIT",
	"private": false,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"predeploy": "npm run build",
		"deploy": "rm -rf /var/www/nacc-home/* && cp -r public/* /var/www/nacc-home"
	},
	"dependencies": {
		"@internationalized/date": "^3.10.1",
		"@sentry/svelte": "^10.32.1",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/enhanced-img": "^0.9.2",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25",
		"bits-ui": "^2.15.4",
		"clsx": "^2.1.1",
		"dotenv": "^17.2.3",
		"globals": "^17.0.0",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"vite": "^8.0.0-beta.0"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.2",
		"@lucide/svelte": "^0.561.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"prettier": "^3.7.4",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"typescript-eslint": "^8.44.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/astutespruce/nacc-home"
	},
	"bugs": {
		"url": "https://github.com/astutespruce/nacc-home/issues"
	}
